<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxl.job.admin.dao.XxlJobTimeZoneDao">
	
	<resultMap id="XxlJobTimeZone" type="com.xxl.job.admin.core.model.XxlJobTimeZone" >
		<result column="id" property="id" />
	    <result column="zone_id" property="zoneId" />
	    <result column="name" property="name" />
	</resultMap>

	<sql id="Base_Column_List">
		t.id,
		t.zone_id,
		t.name
	</sql>

	<select id="findAll" resultMap="XxlJobTimeZone">
		SELECT <include refid="Base_Column_List" />
		FROM xxl_job_time_zone AS t
		ORDER BY id asc
	</select>
</mapper>